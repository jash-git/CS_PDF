function GetPrintVersion(){var n,i,r,t;if(document.getElementById!=null){n="<html><head>";try{n+="<title>"+document.querySelectorAll("head title")[0].innerHTML+"<\/title>";n+='<link href="/css/contentPrint" rel="stylesheet"/>';n+='<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">';n+="<\/head><body>"}catch(u){}if(i=document.getElementById("div1"),r=document.getElementById("div2"),typeof i=="undefined"){alert("could not find the header of the article");return}if(typeof r=="undefined"){alert("oops... nothing to print here!");return}n+='<div id="printContent"><div>Source: C# Corner (<a href = "http://www.c-sharpcorner.com">www.c-sharpcorner.com<\/a>) <input type="button" value="Print" onclick = "print();"> <\/div>';n+="<div>"+i.innerHTML+"<\/div><br/><br/>";n+="<div>"+r.innerHTML+"<\/div>";n+="<div>Thank you for using C# Corner<\/div>";n+="<\/body><\/html>";t=window.open();t.document.open();t.document.write(n);t.document.close()}else alert("Printing is not supported on this device!")}